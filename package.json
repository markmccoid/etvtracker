{
  "name": "etvtracker",
  "productname": "TV Tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/styled": "^10.0.9",
    "electron-is-dev": "^0.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lodash": "^4.17.10",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-pose": "^2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-rc.1",
    "@emotion/core": "^10.0.9",
    "@reach/tabs": "^0.1.1",
    "@types/lodash": "^4.14.116",
    "alertifyjs": "^1.11.1",
    "antd": "^3.6.6",
    "axios": "^0.18.0",
    "concurrently": "^3.6.0",
    "devtron": "^1.4.0",
    "dotenv": "^6.0.0",
    "electron": "^11.5.0",
    "electron-builder": "^20.19.1",
    "emotion": "^9.2.12",
    "firebase": "^5.2.0",
    "moment": "^2.22.2",
    "react-app-rewired": "^1.5.2",
    "react-beautiful-dnd": "^8.0.3",
    "react-emotion": "^9.2.12",
    "react-inspector": "^3.0.2",
    "react-json-view": "^1.19.1",
    "react-powerplug": "^1.0.0-rc.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-freeze": "^0.1.5",
    "redux-thunk": "^2.3.0",
    "sweetalert": "^2.1.0",
    "sweetalert2": "^7.25.0",
    "theming": "^1.3.0",
    "toml": "^2.3.3",
    "wait-on": "^2.1.0"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-app-rewired start",
    "start2": "react-scripts start",
    "electron-dev": "concurrently  \"yarn start2\" \"wait-on http://localhost:3000 && electron .",
    "build": "react-scripts build",
    "electron-pack": "build --em.main=build/electron.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "docs": "jsdoc2md src/datamodel/*.js > datamodel.md"
  },
  "build": {
    "appId": "com.mccoidco.tvtracker",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "assets/favicon.ico"
    }
  },
  "scriptsOld": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
